@ARTICLE{Bhaduri2018-nt,
  title    = "{Identification of cell types in a mouse brain single-cell atlas
              using low sampling coverage}",
  author   = "Bhaduri, Aparna and Nowakowski, Tomasz J and Pollen, Alex A and
