  month     =  jul,
  year      =  2018,
  url       = "https://doi.org/10.1080/10618600.2017.1366914",
  keywords  = "visualisation;[SinaPlot];CITED;PhD analysis methods",
  issn      = "1061-8600",
  doi       = "10.1080/10618600.2017.1366914"
}

@ARTICLE{Lex2014-hy,
