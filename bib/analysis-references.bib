  keywords = "visualisation;[UpSet];UNREAD;PhD analysis methods",
  language = "en",
  issn     = "1077-2626, 1941-0506",
  pmid     = "26356912",
  doi      = "10.1109/TVCG.2014.2346248",
  pmc      = "PMC4720993"
}

